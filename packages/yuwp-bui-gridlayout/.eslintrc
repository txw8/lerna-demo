{
  "env": {
    "mocha": true
  },
  "globals": {
    "Promise": false,
    "yufp": false,
    "expect": true,
    "sinon": true
  },
  "plugins": ["html", "json"],
  "extends": "elemefe",
  "rules": {
    "no-restricted-globals": ["error", "event", "fdescribe"],
    "no-unused-vars": ["error", { "args": "none" }],
    "no-debugger": 1,
    "no-multi-str": 0,
    "no-eval": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  }
}